{
    "name": "ret-cms-backend",
    "version": "1.0.0",
    "description": "",
    "private": true,
    "egg": {
        "typescript": true,
        "declarations": true
    },
    "scripts": {
        "docker-run": "docker-compose -f docker/docker-compose.yml up",
        "docker-stop": "docker-compose -f docker/docker-compose.yml  down",
        "start": "egg-scripts start --daemon --title=egg-server-ret-cms-backend",
        "stop": "egg-scripts stop --title=egg-server-ret-cms-backend",
        "dev": "egg-bin dev",
        "debug": "egg-bin debug",
        "test-local": "egg-bin test",
        "test": "npm run lint -- --fix && npm run test-local",
        "cov": "egg-bin cov",
        "tsc": "ets && tsc -p tsconfig.json",
        "ci": "npm run lint && npm run cov && npm run tsc",
        "autod": "autod",
        "lint": "tslint --project . -c tslint.json",
        "lint-fix": "tslint --project . -c tslint.json --fix",
        "clean": "ets clean",
        "commit": "git-cz"
    },
    "dependencies": {
        "egg": "^2.6.1",
        "egg-scripts": "^2.6.0",
        "tslint-config-egg": "^1.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "@types/mocha": "^2.2.40",
        "@types/node": "^7.0.12",
        "@types/supertest": "^2.0.0",
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
        "commitizen": "^4.0.3",
        "commitlint": "^8.1.0",
        "cz-conventional-changelog": "^3.0.2",
        "egg-bin": "^4.11.0",
        "egg-ci": "^1.8.0",
        "egg-mock": "^3.16.0",
        "husky": "^3.0.5",
        "tslib": "^1.9.0",
        "tslint": "^5.20.0",
        "tslint-config-egg": "^1.0.0",
        "typescript": "^3.0.0"
    },
    "husky": {
        "hooks": {
            "pre-push": "",
            "pre-commit": "npm run lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "engines": {
        "node": ">=10.13.0"
    },
    "ci": {
        "version": "8"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "eslintIgnore": [
        "coverage"
    ],
    "author": "denlly <fan91163@gmail.com>",
    "license": "MIT",
    "config": {
        "commitizen": {
            "path": "./backend/node_modules/cz-conventional-changelog"
        }
    }
}
